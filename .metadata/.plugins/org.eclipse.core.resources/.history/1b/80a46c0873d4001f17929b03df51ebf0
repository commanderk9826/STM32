#include "app.h"

extern UART_HandleTypeDef huart2;

int cycle300ms;
int cycle380ms;


//interrupt에 의하여 1ms마다 호출됨
void SystickCallback(){
	if(cyclee300ms>0) cycle300ms--;
	if(cyclee380ms>0) cycle380ms--;
}


//main
void app(){
	//uart 장치 초기화
	initUart(&huart2);
	while(1){
		if(cycle300ms==0){
			cycle300ms=300;
		}

	}
}

