#include "app.h"
#include <string.h>
#include <stdio.h>

extern UART_HandleTypeDef huart2;

int _write(int file, char*p, int len){
	HAL_UART_Transmit(&huart2, (uint8_t*)p, len, 10);
	return len;
}

void app(){
	static int angle=0;
	angle++;
	angle%=360;
	int sin = sin(angle*M_PI/180)*100+100;
	printf("%d\n", sin);

}
