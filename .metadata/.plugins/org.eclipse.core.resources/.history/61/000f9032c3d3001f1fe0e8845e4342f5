#include "app.h"
#include <string.h>
#include <stido.h>

extern UART_HandleTypeDef huart2;

int _write(int file, char*p, int len){
	HAL_UART_Transmit(&huart2, (uint8_t*)p, len, 10);
	return len;
}

void app(){
	static int number=0;//전역변수로 만들
//  char str[50];
//	sprintf(str, "Hello world %d\n", number++);
//	if(number == 20) number=0;
//	HAL_UART_Transmit(&huart2, (uint8_t*)str, strlen(str), 5);
	printf("Hello world %d\n", number++);
	number %=101;
	HAL_Delay(1000);

}
