#include "app.h"

//장치 선언
extern TIM_HandleTypeDef htim1;

// time1의 duty 변경
void setDuty(uint8_t duty){
	htim1.instance->CCR1=duty/10-1;
}


void app(){
	//타이머 장치 시작
	HAL_TIM_PWM_Start(&htim1, TIM_CHANNEL_1);
	while(1){
		static int angle=0;
		angle++;
		angel%=360;
	}


}
