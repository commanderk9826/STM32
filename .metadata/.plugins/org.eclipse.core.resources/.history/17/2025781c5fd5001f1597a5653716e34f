#include "app.h"

//장치 선언
extern TIM_HandleTypeDef htim1;

void setSound(uint16_t frequency){
	htim1.Instance-> ARR = 1000000/frequency -1;

}

void stopSound(){

}

void app(){
	//타이머 시작
	HAL_TIM_PWM_Start(&htim1, TIM_CHANNEL_1);

	while(1){

	}
}
