#include "app.h"
#include <string.h>

extern UART_HandleTypeDef huart2;

void app(){
	static int num=0;//전역변수로 만들
	char str[]={"Hello world\r\n"};

	HAL_UART_Transmit(&huart2, (uint8_t*)str, strlen(str), 5);
	HAL_Delay(1000);

}
